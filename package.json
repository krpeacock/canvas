{
  "name": "canvas_assets",
  "version": "0.1.0",
  "description": "Internet Computer starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "type": "module",
  "scripts": {
    "build": "webpack",
    "start": "webpack serve --mode development --env development",
    "test": "jest",
    "copy:types": "rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/** --exclude='assets/' --exclude='idl/' --exclude='*.wasm' --delete src/declarations",
    "mint:dialectic": "node --es-module-specifier-resolution=node src/minting/dialectic.js"
  },
  "devDependencies": {
    "@dfinity/assets": "^0.21.4",
    "@types/node": "^20.11.16",
    "@types/react-color": "^3.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "assert": "2.1.0",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "^12.0.2",
    "events": "3.3.0",
    "html-webpack-plugin": "5.6.0",
    "isomorphic-fetch": "^3.0.0",
    "prettier": "^3.2.5",
    "process": "0.11.10",
    "sha256-file": "^1.0.0",
    "stream-browserify": "3.0.0",
    "terser-webpack-plugin": "5.3.10",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "util": "0.12.5",
    "webpack": "5.90.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
  "dependencies": {
    "@adobe/react-spectrum": "^3.33.1",
    "@dfinity/agent": "0.21.4",
    "@dfinity/auth-client": "^0.21.4",
    "@dfinity/candid": "0.21.4",
    "@dfinity/identity": "^0.21.4",
    "@dfinity/principal": "0.21.4",
    "@spectrum-icons/workflow": "^4.2.8",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.54",
    "@types/react-dom": "^18.2.18",
    "bip39": "^3.1.0",
    "css-loader": "^6.10.0",
    "dotenv": "^16.4.5",
    "hdkey": "^2.1.0",
    "jest": "^29.7.0",
    "local-storage": "^2.0.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.0",
    "style-loader": "^3.3.4",
    "styled-components": "^6.1.8",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
